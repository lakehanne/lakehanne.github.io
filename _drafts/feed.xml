<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
<title>Olalekan Ogunmolu</title>
 <link href="http://pat.olalekan-ogunmolu.com/atom.xml" rel="self"/>
 <link href="http://pat.olalekan-ogunmolu.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://pat.olalekan-ogunmolu.com/</id>
 <author>
   <name>Olalekan Ogunmolu</name>
   <email>lexilighty@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://pat.olalekan-ogunmolu.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://pat.olalekan-ogunmolu.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
  </channel>
</rss>
